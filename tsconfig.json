{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["cypress"]
    },
    "include": [
      "cypress/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  